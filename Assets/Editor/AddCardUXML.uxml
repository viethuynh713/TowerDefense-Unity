    </ui:VisualElement>
    <ui:Button text="Add" display-tooltip-when-elided="true" name="AddButton" style="display: flex; width: 100px; height: 30px; white-space: nowrap; -unity-text-align: middle-center; align-items: stretch; left: 40%; right: auto; font-size: 15px; -unity-font-style: bold; bottom: auto;" />
</ui:UXML>
